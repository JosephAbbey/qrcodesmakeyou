{
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "experimentalTernaries": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "bracketSameLine": false,
  "embeddedLanguageFormatting": "auto",
  "endOfLine": "lf",
  "printWidth": 80,
  "proseWrap": "always",
  "plugins": [
    "prettier-plugin-prisma",
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}

{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "short_name": "QRCodes",
  "name": "qrcodesmakeyou",
  "start_url": "/",
  "icons": [
    {
      "src": "logo512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "background_color": "#3367D6",
  "display": "standalone",
  "scope": "/",
  "theme_color": "#3367D6",
  "description": "Professional QRCodes made easy.",
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "title": "shared_title",
      "text": "shared_text",
      "url": "shared_url"
    }
  }
}
